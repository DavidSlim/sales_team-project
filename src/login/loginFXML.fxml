<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="vbMain" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="login.loginCtrl">
    <children>
        <Label alignment="CENTER" prefHeight="44.0" prefWidth="363.0" text="LOGIN FORM">
            <font>
                <Font name="Franklin Gothic Medium" size="25.0" />
            </font>
        </Label>
        <Separator prefHeight="4.0" prefWidth="363.0" />
        <HBox alignment="CENTER" prefHeight="90.0" prefWidth="334.0" spacing="10.0">
            <children>
                <GridPane prefHeight="79.0" prefWidth="148.0" vgap="10.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label prefHeight="18.0" prefWidth="152.0" text="Login Name" />
                        <Label prefHeight="18.0" prefWidth="152.0" text="Login Password" GridPane.rowIndex="1" />
                    </children>
                </GridPane>
                <Separator orientation="VERTICAL" prefHeight="90.0" prefWidth="0.0" />
                <GridPane prefHeight="54.0" prefWidth="160.0" vgap="10.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <TextField fx:id="txtLogName" prefHeight="0.0" prefWidth="160.0" />
                        <PasswordField fx:id="txtLogPass" prefHeight="25.0" prefWidth="171.0" GridPane.rowIndex="1" />
                    </children>
                </GridPane>
            </children>
        </HBox>
        <Separator prefHeight="4.0" prefWidth="349.0" />
        <HBox alignment="CENTER" prefHeight="29.0" prefWidth="334.0" spacing="50.0">
            <children>
                <Button fx:id="btLogin" mnemonicParsing="false" prefHeight="25.0" prefWidth="106.0" text="LOG In" />
                <Separator orientation="VERTICAL" prefHeight="29.0" prefWidth="0.0" />
                <Button fx:id="btLogout" mnemonicParsing="false" prefHeight="25.0" prefWidth="118.0" text="LOG OUt" />
            </children>
        </HBox>
        <Separator prefHeight="4.0" prefWidth="349.0" />
        <HBox alignment="CENTER" spacing="20.0" />
    </children>
</VBox>
