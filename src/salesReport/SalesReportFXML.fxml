                <TextField fx:id="tfSearch" prefHeight="56.0" prefWidth="421.0">
                    <font>
                        <Font name="Arial Black" size="25.0" />
                    </font>
                </TextField>
                <VBox spacing="10.0">
                    <children>
                        <DatePicker fx:id="dpStart" promptText="Start Date" />
                        <DatePicker fx:id="dpEnd" promptText="End Date" />
                    </children>
                </VBox>
                <Button fx:id="btPrint" contentDisplay="RIGHT" mnemonicParsing="false" prefHeight="56.0" prefWidth="331.0" text="GENERATE REPORT">
                    <font>
                        <Font name="Arial Bold" size="23.0" />
                    </font>
                </Button>
            </children>
        </HBox>
    </children>
</AnchorPane>
