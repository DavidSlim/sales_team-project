<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="685.0" prefWidth="955.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="salesReport.SalesCtrlr">
    <children>
        <VBox fx:id="vbMain" layoutY="14.0" prefHeight="605.0" prefWidth="955.0" spacing="15.0">
            <children>
                <Label alignment="CENTER" prefHeight="56.0" prefWidth="480.0" text="SALES REPORT FORM">
                    <font>
                        <Font name="System Bold" size="39.0" />
                    </font>
                </Label>
                <HBox prefHeight="34.0" prefWidth="733.0">
                    <children>
                        <Label fx:id="lblTime" prefHeight="34.0" prefWidth="132.0" text="Label" textFill="#c60f0f">
                            <font>
                                <Font name="Bell MT" size="18.0" />
                            </font>
                        </Label>
                        <Label fx:id="lblDate" alignment="CENTER_RIGHT" prefHeight="33.0" prefWidth="350.0" text="Label" textFill="#230cb9">
                            <font>
                                <Font name="Baskerville Old Face" size="18.0" />
                            </font>
                        </Label>
                    </children>
                </HBox>
                <TableView fx:id="table" prefHeight="480.0" prefWidth="962.0" />
            </children>
        </VBox>
        <HBox layoutY="619.0" prefHeight="56.0" prefWidth="961.0" spacing="30.0">
